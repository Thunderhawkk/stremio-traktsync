{
  "name": "stremio-trakt-multitenant-addon",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec ts-node src/index.ts",
    "dev:js": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:backend": "tsc",
    "build:frontend": "cd dashboard-ui && npm ci && npm run build",
    "build:watch": "tsc --watch",
    "migrate": "ts-node src/db/pg.ts migrate",
    "migrate:js": "node src/db/pg.js migrate",
    "seed": "ts-node src/db/pg.ts seed",
    "seed:js": "node src/db/pg.js seed",
    "db:optimize": "node -e \"require('./src/db/optimizedQueries').cleanupExpiredTokens().then(() => console.log('Optimization completed'))\"",
    "db:stats": "node -e \"require('./src/db/optimizedQueries').getUserStatistics().then(stats => console.log(JSON.stringify(stats, null, 2)))\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "docker:build": "docker build -t stremio-trakt-multiuser .",
    "docker:run": "docker run -p 8080:8080 stremio-trakt-multiuser",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "docker-compose up --build -d",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "logs:create": "mkdir -p logs",
    "postinstall": "npm run logs:create",
    "health-check": "curl -f http://localhost:8080/healthz || exit 1"
  },
  "dependencies": {
    "@types/ws": "^8.18.1",
    "apicache": "^1.6.3",
    "argon2": "^0.44.0",
    "axios": "^1.7.4",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-oauth-server": "^2.0.0",
    "express-rate-limit": "^7.4.0",
    "express-session": "^1.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "oauth2-server": "^3.1.1",
    "p-queue": "^6.6.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "stremio-addon-sdk": "^1.6.9",
    "uuid": "^9.0.1",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-github2": "^1.2.9",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.15.5",
    "@types/pino": "^7.0.4",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.8.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "style-dictionary": "^5.0.4",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
